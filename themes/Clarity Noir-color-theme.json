{
	"name": "Clarity Noir",
	"colors": {
		// Activity bar
		"activityBar.background": "#373737",
		"activityBar.border": "#000000",
		"activityBar.activeBorder": "#dcdcdc",
		"activityBar.dropBorder": "#027aff",
		"activityBar.foreground": "#dcdcdc",
		"activityBar.inactiveForeground": "#8c8c8c",
		"activityBarBadge.background": "#027aff",
		"activityBarBadge.foreground": "#ffffff",
		// Buttons
		"button.background": "#027aff",
		"button.foreground": "#ffffff",
		"button.hoverBackground": "#0069e0",
		"button.secondaryBackground": "#373737",
		"button.secondaryForeground": "#dcdcdc",
		"button.secondaryHoverBackground": "#515151",
		// Checkboxes
		"checkbox.background": "#373737",
		"checkbox.foreground": "#dcdcdc",
		"checkbox.selectBackground": "#515151",
		"checkbox.selectBorder": "#027aff",
		// Dropdowns
		"dropdown.background": "#373737",
		"dropdown.foreground": "#dcdcdc",
		"dropdown.listBackground": "#373737",
		// Editor
		"editor.background": "#1e1e1e",
		"editorCursor.foreground": "#dcdcdc",
		"editor.selectionBackground": "#62a9fc80",
		"editor.hoverHighlightBackground": "#62a9fc80",
		"editor.foreground": "#dcdcdc",
		"editorIndentGuide.background1": "#373737",
		"editorIndentGuide.activeBackground1": "#515151",
		"editorLineNumber.foreground": "#686868",
		"editorLineNumber.activeForeground": "#dcdcdc",
		"editorStickyScroll.shadow": "#000000",
		// Editor groups
		"editorGroup.border": "#000000",
		"editorGroup.emptyBackground": "#1e1e1e",
		"editorGroup.dropBackground": "#62a9fc80",
		"editorGroup.dropIntoPromptBackground": "#1e1e1e",
		"editorGroup.dropIntoPromptForeground": "#dcdcdc",
		// Editor hints
		"editorBracketMatch.border": "#ffffff40",
		"editorCodeLens.foreground": "#8c8c8c",
		"editorError.foreground": "#ff3b11",
		"editorLightBulb.foreground": "#ffcc00",
		"editorWarning.foreground": "#ffcc00",
		// Panels
		"panel.background": "#282828",
		"panel.border": "#000000",
		"panel.dropBorder": "#027aff",
		"panelSection.dropBackground": "#62a9fc80",
		"panelSection.border": "#000000",
		"panelSectionHeader.background": "#373737",
		"panelSectionHeader.foreground": "#dcdcdc",
		// Settings
		"settings.modifiedItemIndicator": "#027aff",
		// Sidebar
		"sideBar.border": "#000000",
		"sideBar.background": "#282828",
		"sideBar.foreground": "#dcdcdc",
		"sideBar.dropBackground": "#62a9fc80",
		"sideBarSectionHeader.background": "#ffffff1c",
		"sideBarSectionHeader.foreground": "#dcdcdc",
		// Status bar
		"statusBar.background": "#1e1e1e",
		"statusBar.debuggingBackground": "#b051de",
		"statusBarItem.hoverBackground": "#ffffff1c",
		"statusBarItem.activeBackground": "#515151",
		"statusBarItem.errorBackground": "#ff3b11",
		"statusBarItem.warningBackground": "#ffcc00",
		"statusBarItem.warningForeground": "#1e1e1e",
		"statusBarItem.warningHoverBackground": "#e0b400",
		"statusBarItem.warningHoverForeground": "#282828",
		// Tabs
		"editorGroupHeader.tabsBackground": "#1e1e1e",
		"tab.border": "#000000",
		"tab.activeBackground": "#1e1e1e",
		"tab.activeBorderTop": "#dcdcdc",
		"tab.activeForeground": "#dcdcdc",
		"tab.hoverForeground": "#8c8c8c",
		"tab.inactiveBackground": "#1e1e1e",
		"tab.inactiveForeground": "#686868",
	},
	"tokenColors": [
		{
			"name": "Blockquote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#8c8c8c"
			}
		},
		{
			"name": "Blockquote > Punctuation",
			"scope": "markup.quote punctuation.definition",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Bold",
			"scope": "markup.bold",
			"settings": {
				"foreground": "#02c7be",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Bold > Punctuation",
			"scope": "punctuation.definition.bold",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Bolded Italic",
			"scope": "markup.bold markup.italic",
			"settings": {
				"foreground": "#02c7be",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "Code",
			"scope": "markup.inline.raw",
			"settings": {
				"foreground": "#e08300"
			}
		},
		{
			"name": "Code > Punctuation",
			"scope": "punctuation.definition.raw",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment.block",
				"comment.line"
			],
			"settings": {
				"foreground": "#8c8c8c"
			}
		},
		{
			"name": "Comments > Punctuation",
			"scope": "punctuation.definition.comment",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Dictionaries > Punctuation",
			"scope": [
				"punctuation.definition.dictionary",
				"punctuation.separator.dictionary.key-value",
				"punctuation.separator.dictionary.pair",
				"punctuation.separator.key-value"
			],
			"settings": {
				"foreground": "#8c8c8c"
			}
		},
		{
			"name": "Heading > Level 1",
			"scope": "heading.1",
			"settings": {
				"foreground": "#ff3b11"
			}
		},
		{
			"name": "Heading > Level 2",
			"scope": "heading.2",
			"settings": {
				"foreground": "#ff9502"
			}
		},
		{
			"name": "Heading > Level 3",
			"scope": "heading.3",
			"settings": {
				"foreground": "#ffcc00"
			}
		},
		{
			"name": "Heading > Level 4",
			"scope": "heading.4",
			"settings": {
				"foreground": "#2acd41"
			}
		},
		{
			"name": "Heading > Level 5",
			"scope": "heading.5",
			"settings": {
				"foreground": "#027aff"
			}
		},
		{
			"name": "Heading > Level 6",
			"scope": "heading.6",
			"settings": {
				"foreground": "#b051de"
			}
		},
		{
			"name": "Heading > Punctuation",
			"scope": "punctuation.definition.heading",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Italic",
			"scope": "markup.italic",
			"settings": {
				"foreground": "#b0e6e3",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Italic > Punctuation",
			"scope": "punctuation.definition.italic",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Keywords > Constants",
			"scope": "constant.language",
			"settings": {
				"foreground": "#027aff",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Link",
			"scope": [
				"meta.link markup.underline.link",
				"meta.image markup.underline.link"
			],
			"settings": {
				"foreground": "#3392ff",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Link > Alt Text",
			"scope": "meta.image string",
			"settings": {
				"foreground": "#ff2e55"
			}
		},
		{
			"name": "Link > Punctuation",
			"scope": [
				"meta.link punctuation.definition.constant",
				"meta.image punctuation.definition.constant",
				"meta.link punctuation.definition.link",
				"meta.image punctuation.definition.link",
				"meta.link punctuation.definition.metadata",
				"meta.image punctuation.definition.metadata"
			],
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Link > Reference",
			"scope": [
				"meta.link constant.other.reference",
				"meta.image constant.other.reference"
			],
			"settings": {
				"foreground": "#e0b400"
			}
		},
		{
			"name": "Link > Text",
			"scope": "meta.link string.other.link.title",
			"settings": {
				"foreground": "#02c7be"
			}
		},
		{
			"name": "Lists > Punctuation",
			"scope": [
				"punctuation.definition.array",
				"punctuation.definition.sequence",
				"punctuation.separator.array",
				"punctuation.separator.sequence",
			],
			"settings": {
				"foreground": "#8c8c8c"
			}
		},
		{
			"name": "List items > Punctuation",
			"scope": [
				"punctuation.definition.block.sequence.item",
				"punctuation.definition.list"
			],
			"settings": {
				"foreground": "#8c8c8c"
			}
		},
		{
			"name": "Keys",
			"scope": [
				"support.type.property-name",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#02c7be",
			}
		},
		{
			"name": "Numbers",
			"scope": "constant.numeric",
			"settings": {
				"foreground": "#45d958"
			}
		},
		{
			"name": "Strikethrough",
			"scope": "markup.strikethrough",
			"settings": {
				"foreground": "#8c8c8c",
				"fontStyle": "strikethrough"
			}
		},
		{
			"name": "Strikethrough > Punctuation",
			"scope": "punctuation.definition.strikethrough",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "Strings",
			"scope": "string",
			"settings": {
				"foreground": "#e08300"
			}
		},
		{
			"name": "Table > Punctuation",
			"scope": [
				"punctuation.definition.table",
				"punctuation.separator.table"
			],
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "[TOML] Properties > Assignment",
			"scope": "punctuation.eq.toml",
			"settings": {
				"foreground": "#686868"
			}
		},
		{
			"name": "[TOML] Tables > Inline > Punctuation",
			"scope": "punctuation.definition.table.inline.toml",
			"settings": {
				"foreground": "#8C8C8C"
			}
		},
		{
			"name": "[TOML] Tables > Name",
			"scope": "support.type.property-name.table.toml",
			"settings": {
				"foreground": "#c076e5"
			}
		},
		{
			"name": "[TOML] Tables > Name > Punctuation",
			"scope": "punctuation.separator.dot.toml",
			"settings": {
				"foreground": "#8C8C8C"
			}
		}
	]
}
